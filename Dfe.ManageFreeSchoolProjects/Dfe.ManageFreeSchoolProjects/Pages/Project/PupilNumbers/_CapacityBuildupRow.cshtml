@model Dfe.ManageFreeSchoolProjects.Pages.Project.PupilNumbers.CapacityRowBuildupViewModel

<tr class="govuk-table__row">
    <td class="govuk-table__cell">
        @if (Model.BoldLabel)
        {
            <strong>@Model.Label</strong>
        }
        else
        {
            @Model.Label
        }
    </td>
    <td class="govuk-table__cell">@Model.CapacityBuildupEntry.CurrentCapacity</td>
    <td class="govuk-table__cell">@Model.CapacityBuildupEntry.FirstYear</td>
    <td class="govuk-table__cell">@Model.CapacityBuildupEntry.SecondYear</td>
    <td class="govuk-table__cell">@Model.CapacityBuildupEntry.ThirdYear</td>
    <td class="govuk-table__cell">@Model.CapacityBuildupEntry.FourthYear</td>
    <td class="govuk-table__cell">@Model.CapacityBuildupEntry.FifthYear</td>
    <td class="govuk-table__cell">@Model.CapacityBuildupEntry.SixthYear</td>
    <td class="govuk-table__cell">@Model.CapacityBuildupEntry.SeventhYear</td>
</tr>
