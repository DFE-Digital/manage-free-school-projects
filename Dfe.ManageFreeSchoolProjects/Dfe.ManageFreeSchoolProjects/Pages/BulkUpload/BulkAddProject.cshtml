@page "/bulk/upload/addproject"
@model Dfe.ManageFreeSchoolProjects.Pages.BulkUpload.BulkAddProjectModel
@{
}

<form method="post" enctype="multipart/form-data">
    <div class="govuk-form-group">
        <label class="govuk-label" for="file-upload">
            Upload a file
        </label>
        <input class="govuk-file-upload" id="file-upload" name="upload" type="file" accept=".xlsx">
        <button class="govuk-button" data-module="govuk-button">
            Submit
        </button>
    </div>
</form>

