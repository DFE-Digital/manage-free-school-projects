{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "GoogleAnalytics": {
    "Enable": "No"
  },
  "Mfsp": {
    "ApiEndpoint": "<insert api endpoint>",
    "ApiKey": "app-key"
  },
  "AzureAd": {
    "Instance": "https://login.microsoftonline.com",
    "Domain": "Educationgovuk.onmicrosoft.com",
    "CallbackPath": "/signin-oidc",
    "ClientId": "insert-client-id",
    "TenantId": "insert-tenant-id",
    "ClientSecret": "insert-client-secret",
    "GroupId": "insert-group-id",
    "AllowedRoles": "user"
  },
  "ApplicationInsights": {
    "ConnectionString": "secret"
  },
  "CypressTestSecret": "secret",
  "AuthenticationExpirationInMinutes": 60,
  "FeatureManagement": {
    "UseAcademisation": true,
    "UseAcademisationApplication": false
  },
  "ConnectionStrings": {
    "BlobStorage": ""
  }
}
